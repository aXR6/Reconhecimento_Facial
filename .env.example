# Exemplo de configuracao dos modelos utilizados

# Repositório do detector de faces MediaPipe
MEDIAPIPE_REPO=qualcomm/MediaPipe-Face-Detection

# Repositório do detector de faces YOLOv8
YOLOV8_REPO=jaredthejelly/yolov8s-face-detection

# Modelo para gerar legendas de imagens
HF_CAPTION_MODEL=nlpconnect/vit-gpt2-image-captioning

# Modelo responsável por detectar obstrução facial
OBSTRUCTION_MODEL_REPO=dima806/face_obstruction_image_detection

# Repositório do FaceXFormer utilizado nas funções de demografia
FACEXFORMER_REPO=kartiknarayan/facexformer

# Modelo padrão do Whisper usado na tradução de áudio
WHISPER_MODEL=openai/whisper-large-v3-turbo

# String de conexão para o banco PostgreSQL
POSTGRES_DSN=postgresql://user:pass@localhost:5432/face_db

# Diretório local com imagens para o recurso social-search
SOCIAL_DB_PATH=
